{
  "mcpServers": {
    "time": {
      "command": "uvx",
      "deps": [
        "pytz",
        "tzdata"
      ],
      "args": [
        "mcp-server-time",
        "--local-timezone",
        "America/Tijuana"
      ]
    },
    "dbutils": {
      "command": "uvx",
      "args": [
        "mcp-dbutils",
        "--config",
        "/Users/carlos/.config/db/config.yaml"
      ]
    }
  }
}
