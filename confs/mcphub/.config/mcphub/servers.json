{
  "nativeMCPServers": [],
  "mcpServers": {
    "dbutils": {
      "args": [
        "mcp-dbutils",
        "--config",
        "/Users/carlos/.config/db/config.yaml"
      ],
      "command": "uvx"
    },
    "time": {
      "deps": [
        "pytz",
        "tzdata"
      ],
      "args": [
        "mcp-server-time",
        "--local-timezone",
        "America/Tijuana"
      ],
      "command": "uvx"
    },
    "github": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": null
      },
      "disabled": false,
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "command": "docker"
    }
  }
}
